<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"   
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
	xsi:schemaLocation="http://www.springframework.org/schema/beans   
	http://www.springframework.org/schema/beans/spring-beans-4.0.xsd"
	default-init-method="defaultInit" default-destroy-method="defaultDestroy">  
	
	<bean id="beanLifeCycle" class="com.wangzhu.spring.lifecycle.BeanLifeCycle" init-method="start" destroy-method="stop"></bean>
	<bean class="com.wangzhu.spring.Processor.DefaultPostProcessor"/>
</beans>